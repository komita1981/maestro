<!DOCTYPE html>
<html>
<head>
    <title>{% trans %}Sider - Redis manager{% endtrans %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link rel="stylesheet" type="text/css" media="all" href="/bundles/komitasider/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" media="all" href="/bundles/komitasider/css/jumbotron.css">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/komitasider/js/html5shiv.js') }}"></script>
    <script src="{{ asset('bundles/komitasider/js/respond.min.js') }}"></script>
    <![endif]-->
</head>
<body>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('komita_sider_sider_homepage') }}">SIDER</a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li class="active"><a href="{{ path('komita_sider_sider_homepage') }}">{% trans %}Home{% endtrans %}</a></li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{% trans %}Configuration{% endtrans %} <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="#">Select</a></li>
                        <li><a href="#">Edit</a></li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{% trans %}Insert{% endtrans %} <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="#">String</a></li>
                        <li><a href="#">Hash</a></li>
                        <li><a href="#">Set</a></li>
                        <li><a href="#">Sorted Set</a></li>
                        <li><a href="#">List</a></li>
                    </ul>
                </li>
            </ul>
            {{ form_start(search_form, {'attr': {'class': 'navbar-form navbar-right' } } ) }}
                <div class="form-group">
                    {{ form_widget(search_form.key, {'attr': {'class': 'form-control',
                                                               'placeholder': 'Enter key name',
                                                               'data-toggle': 'tooltip',
                                                               'data-placement': 'bottom',
                                                               'data-original-title': 'Enter part of the key name' } } ) }}
                </div>
                <div class="form-group">
                    {{ form_widget(search_form.datatype, {'attr': {'class': 'form-control',
                                                                    'data-toggle': 'tooltip',
                                                                    'data-placement': 'bottom',
                                                                    'data-original-title': 'Select datatype' } } ) }}
                </div>
            <div class="form-group">
                    {{ form_widget(search_form.database, {'attr': {'class': 'form-control',
                    'data-toggle': 'tooltip',
                    'data-placement': 'bottom',
                    'data-original-title': 'Select database' } } ) }}
                </div>
                {{ form_widget(search_form.search, {'attr': {'class': 'btn btn-success', 'placeholder': 'Search' } } ) }}
                {{ form_end(search_form) }}
        </div>
        <!--/.navbar-collapse -->
    </div>
</div>

{% block body %}{% endblock %}

<div class="container">
    <!-- /container -->
    <hr>

    <footer>
        <p>&copy; Company 2013</p>
    </footer>
</div>
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="{{ asset('bundles/komitasider/js/jquery.min.js') }}"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="{{ asset('bundles/komitasider/js/bootstrap.min.js') }}"></script>
<script type="text/javascript">
    $('#search_keys_key').tooltip();
    $('#search_keys_datatype').tooltip();
    $('#search_keys_database').tooltip();
</script>
</body>
</html>